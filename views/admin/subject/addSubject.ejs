<%- include('../../partial/admin/header') %>

<div class="container">
  <h2>Add Subject</h2>
  <div class="text-right">
    <a href="/admin/test/subject" class="btn btn-primary">Back to All Subject</a>

</div>
  <form action="/admin/test/addSubject" method="POST" enctype="multipart/form-data">
    <input type="hidden" name="_csrf" value="<%= csrfToken %>">
   
    <div class="form-group">
      <label for="email">title:</label>
      <input type="text" class="form-control"  value="<%= title %>" name="title" id="title" />
    </div>
    
   
    <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>
<%- include('../../partial/admin/footer') %>
