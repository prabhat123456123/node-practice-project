<%- include('../../partial/admin/header') %>

<div class="container">
  
  <h2>Add Category</h2>
  <div class="text-right">
    <a href="/admin/category" class="btn btn-primary">Back to Category Page</a>

</div>
  <form action="/admin/category/postAddCategory" method="post">
    <input type="hidden" name="_csrf" value="<%= csrfToken %>">

    <div class="form-group">
      <label for="email">Category Name:</label>
      <input
        type="text"
        class="form-control"
        value="<%= title %>"
        name="title"
        id="title"
      />
    </div>

    <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>
<%- include('../../partial/admin/footer') %>
